<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="ScanMyListAzure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-10.1.8">
  <WebRole name="SynchWebRole" vmsize="Small">
    <Certificates>
      <!--Certificate name="SMLTest" storeLocation="LocalMachine" storeName="CA"/-->
    </Certificates>
    <Sites>
      <Site name="Web">
        <Bindings>
          <!--Binding name="HttpsIn" endpointName="HttpsIn" /-->
          <Binding name="Http" endpointName="Http" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Http" protocol="http" port="80" />
      <!--InputEndpoint name="HttpsIn" protocol="https" port="443" certificate="SMLTest" /-->
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <LocalResources>
      <LocalStorage name="SynchWebRole.svclog" sizeInMB="1000" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings>
      <Setting name="SynchStorageConnection" />
    </ConfigurationSettings>
  </WebRole>
  <WorkerRole name="ERPIntegrationWorkerRole" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="SynchStorageConnection" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>