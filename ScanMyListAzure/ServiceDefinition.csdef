<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="ScanMyListAzure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-10.1.8">
  <WebRole name="ScanMyListWebRole" vmsize="Small">
    <Certificates>
    </Certificates>
    <Sites>
      <Site name="Web">
        <Bindings>
          <!--Binding name="HttpsIn" endpointName="HttpsIn" /-->
          <Binding name="Http" endpointName="Http"/>
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Http" protocol="http" port="80"/>
      <!--InputEndpoint name="HttpsIn" protocol="https" port="443" certificate="synchtest" /-->
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <LocalResources>
      <LocalStorage name="ScanMyListWebRole.svclog" sizeInMB="1000" cleanOnRoleRecycle="false" />
    </LocalResources>
  </WebRole>
</ServiceDefinition>